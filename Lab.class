package task3;

import java.io.PrintStream;
import java.util.ArrayList;
import java.util.List;

public class Lab1 {
    public Lab1() {
    }

    public static void main(String[] var0) {
        List var1 = createThreePoints(var0);
        if (!((Point3d)var1.get(0)).equals(var1.get(1)) && !((Point3d)var1.get(1)).equals(var1.get(2)) && !((Point3d)var1.get(2)).equals(var1.get(0))) {
            PrintStream var10000 = System.out;
            Point3d var10001 = (Point3d)var1.get(0);
            var10000.println("Area = " + computeArea(var10001, (Point3d)var1.get(1), (Point3d)var1.get(2)));
        } else {
            System.out.println("Two points are equal");
        }

    }

    public static List<Point3d> createThreePoints(String[] var0) {
        ArrayList var1 = new ArrayList();

        for(int var2 = 0; var2 < var0.length; var2 += 3) {
            var1.add(new Point3d(Double.parseDouble(var0[var2]), Double.parseDouble(var0[var2 + 1]), Double.parseDouble(var0[var2 + 2])));
        }

        return var1;
    }

    public static double computeArea(Point3d var0, Point3d var1, Point3d var2) {
        double var3 = var0.distanceTo(var1);
        double var5 = var1.distanceTo(var2);
        double var7 = var2.distanceTo(var0);
        double var9 = (var3 + var5 + var7) / 2.0D;
        return Math.sqrt(var9 * (var9 - var3) * (var9 - var5) * (var9 - var7));
    }
}
